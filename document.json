{
	"name": "document",
	"description": "Document template for Cogneco.",
	"output": "html",
	"dependencies": [
	],
	"tags": {
		"document": "<!doctype html>\n<html>\n<head>\n<meta charset='UTF-8'>\n<title>${title}</title>\n<meta name='author' content='${author}'/>\n<meta name='date' content='${date}'/>\n<meta name='identification' content='${identification}'/>\n<meta name='classification' content='${classification}'/>\n<link rel='stylesheet' type='text/css' href='${template-path}/${class}.css'>\n<script src='${template-path}/toc.js' type='text/javascript'></script>\n</head>\n<body>\n<header><h1>${title}</h1></header>\n<nav></nav>\n${content}</body>\n</html>\n",
		"heading": "<h${level}>${content}</h${level}>\n",
		"paragraph": "<p>${content}</p>\n",
		"list item": "<li>${content}</li>\n",
		"unordered list": "<ul>\n${content}</ul>\n",
		"ordered list": "<ol>\n${content}</ol>\n",
		"emphasize": "<em>${content}</em>",
		"codeblock": "<pre><code>${content}</code></pre>",
		"code": "<code>${content}</code>",
		"link": "<a href='${target}'>${content}</a>",
		"*": "<h1><code>${content}</code></h1>"
	}
}
